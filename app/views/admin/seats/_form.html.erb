<%= form_for [:admin, @seats] do |f| %>
<div class="form-group">
  <%= f.label :num_of_row %>
  <%= f.text_field :num_of_row, class: "form-control" %>
</div>

<div class="form-group">
  <%= f.label :num_of_collum %>
  <%= f.text_field :num_of_collum ,class: "form-control" %>
</div>

<div class="form-group">
  <%= f.label :seat_type %>
  <%= f.text_field :seat_type ,class: "form-control" %>
</div>

<div class="form-group">
  <%= f.label :cinemaroom_id %>
   <%= f.select :cinemaroom_id, @list_cinemaroom,
      {include_blank: "Choose one",selected: @seats.cinemaroom_id},
      {class: "form-control"} %>
</div>

<%= f.label :status %>
<div class="form-group">
  <%= label :status, t("action") %>
  <%= f.radio_button :status, "1",:checked => true %>
  <%= label :status, t("unaction") %>
  <%= f.radio_button :status, "0" %>
</div>

<%= f.submit yield(:button_text), class: "btn btn-primary" %>
<% end %>
