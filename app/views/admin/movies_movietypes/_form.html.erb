<%= form_for [:admin, @movies_movietypes], remote: true do |f| %>
<%= render "shared/error_messages", :target => @cinemaroom %>
<div class="form-group">
  <%= f.label :movie_id %>
  <%= f.text_field :movie_id, class: "form-control" %>
</div>

<div class="form-group">
  <%= f.label :movietype_id %>
  <%= f.text_field :movietype_id, class: "form-control" %>
</div>

<%= f.submit yield(:button_text), class: "btn btn-primary" %>
<% end %>
