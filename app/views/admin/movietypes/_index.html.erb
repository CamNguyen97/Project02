 <% @movietypes.each_with_index do |item, index| %>
  <tr>
    <td class="text-center"><%= Index index %></td>
    <td><%= item.name %></td>
    <td class="text-center"><%= item.status %></td>
    <td>
     <%= link_to t("edit"), new_admin_movietype_path(item), remote: true, class:"btn btn-info" %>
    <%= link_to t("delete"), admin_movietype_delete_path(item), remote: true, class:"btn btn-danger" %>
    </td>
  </tr>
<% end %>