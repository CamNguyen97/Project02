<% @schedules.each do |item| %>
  <tr>
    <td><%= item.id %></td>
    <td><%= item.show_case %></td>
    <td><%= item.date_movie %></td>
    <td><%= item.status %></td>
    <td><%= Movie.find_by(id: item.movie_id).name %></td>
    <td>
      <%= link_to t("edit"), edit_admin_schedule_path(item), remote: true, class:"btn btn-info" %>
    <%= link_to t("delete"), admin_schedule_delete_path(item), remote: true, class:"btn btn-danger" %>
    </td>
  </tr>
<% end %>